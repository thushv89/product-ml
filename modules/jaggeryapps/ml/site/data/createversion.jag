<script type="text/javascript">

function displayDataSource() {
        var dataSourceType = $('#src-type option:selected').text();
        if (dataSourceType == 'HDFS') {
	        $('#source-file').hide();
	        $('#size-limit').hide();
	        $('#source-hdfs-bam').html('<input type="text" value="" placeholder="e.g. hdfs://localhost:9000/home/datasets/iris.csv" id="src-path" name="sourcePath" />');
	        $('#source-hdfs-bam').show();
	        $('#not-related-to-bam').show();
        } else if (dataSourceType == 'BAM') {
            $('#source-file').hide();
            $('#size-limit').hide();
            $('#source-hdfs-bam').html('<input type="text" value="" placeholder="e.g. <table_name>:<col_1_name>,<col_1_type>;<col_2_name>,<col_2_type>" id="src-path" name="sourcePath" />');
            $('#source-hdfs-bam').show();
            $('#not-related-to-bam').hide();
        }
        else {
            $('#source-hdfs-bam').hide();
            $('#size-limit').show();
            $('#not-related-to-bam').show();
            $('#source-file').show();
        }
}

$( document ).ready(function() {
	// Display only the correct dataSource input 
	displayDataSource();

	var baseUrl = getBaseUrl(window.location.href);

	$('#nav').affix({
	      offset: {
	        top: $('header').height()
	      }
	});

	$("[data-toggle=popover]").popover();

	$(".ctrl-asset-type-switcher").popover({
		html : true, 
		content: function() {
			return $('#content-asset-types').html();
        	}
    	});

	// put dataset name to application navigator
	var name = getParameterByName('dsnam');
	$('#nav-dataset').text(name);	

	$('#datasetVersionCreateForm').ajaxForm({
	    data: { 'datasetName': name,
		    'version' : getParameterByName('datasetVersion')
		   },
	    url: baseUrl + '/api/datasets',
	    /*beforeSend: function(xhr) {
	        xhr.setRequestHeader("Authorization", "Basic " + authEncoded);
	    },*/
        success : function(res){
    	     window.location = baseUrl + '/ml/site/data/datasets.jag';   
        },
        error :  function(res){
                var errorText = res.responseText
            handleNotification(errorText, '#notification-area', 'warning');
        }                               
	});

	$('#cancel-version').on('click', function(e){
		window.location.href = './datasets.jag';
	});

	// change data source input according to data source type input
	$('#src-type').on('change', function(e){
		displayDataSource();
	});

}); 

</script>

